QWidget {
	background-color: #1e1e1e;
	color: #ffffff;
}

QLabel {
	color: #ffffff;
}

QComboBox {
	background-color: #2b2b2b;
	color: #ffffff;
}

QComboBox::drop-down {
	border: 0px
}

QListWidget {
	background-color: #2b2b2b;
	color: #ffffff;
}

QListWidget::item:selected {
	background-color: #185fb4;
	color: #ffffff;
}

QScrollBar:vertical, QScrollBar:horizontal {
	background-color: #2b2b2b;
}
